FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -3 0 0 0 0 0 ];

internalField   uniform 8000;

boundaryField
{
    ".*"
    {
        type            calculated;
        value           uniform 8000;
    }
    defaultFaces
    {
        type            empty;
    }
}


